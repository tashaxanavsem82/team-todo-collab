{
  "name": "team-todo-collab",
  "version": "1.0.0",
  "description": "A collaborative todo list application that allows teams to create, assign, and track tasks in real-time.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently 'npm run client' 'nodemon server.js'"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.9",
    "socket.io": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "concurrently": "^5.3.0"
  }
}